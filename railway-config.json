{
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "models": {
    "providers": {
      "atlascloud": {
        "baseUrl": "https://api.atlascloud.ai/v1",
        "apiKey": "${ATLASCLOUD_API_KEY}",
        "api": "openai-completions"
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "atlascloud/qwen3-max-20260123"
      }
    }
  }
}
