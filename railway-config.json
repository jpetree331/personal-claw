{
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "controlUi": {
      "dangerouslyDisableDeviceAuth": true
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "models": {
    "providers": {
      "atlascloud": {
        "baseUrl": "https://api.atlascloud.ai/v1",
        "apiKey": "${ATLASCLOUD_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "qwen3-max-20260123",
            "name": "Qwen3 Max",
            "compat": {}
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "atlascloud/qwen3-max-20260123"
      }
    }
  },
  "plugins": {
    "entries": {
      "openclaw-supermemory": {
        "enabled": true,
        "config": {
          "apiKey": "${SUPERMEMORY_OPENCLAW_API_KEY}"
        }
      }
    }
  },
  "channels": {
    "discord": {
      "enabled": true,
      "token": "${DISCORD_BOT_TOKEN}"
    },
    "whatsapp": {
      "selfChatMode": true,
      "dmPolicy": "allowlist",
      "allowFrom": ["${USER_NUMBER}"]
    }
  }
}
